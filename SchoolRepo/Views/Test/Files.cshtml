
@model SchoolRepo.Models.Test.FilesViewModel

@{
    ViewBag.Title = "Add Student";
}

<p>List of Files</p>

<ul>
    @foreach (var item in Model.Files)
    {
    <li>
        <a asp-action="Download"
           asp-route-filename="@item.Name">
            @item.Name
        </a>
    </li>
    }
</ul>